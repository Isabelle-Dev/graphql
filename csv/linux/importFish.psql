	EndTime character varying(40),
	NHJan character varying(4),
	NHFeb character varying(4),
	NHMar character varying(4),
	NHApr character varying(4),
	NHMay character varying(4),
	NHJun character varying(4),
	NHJul character varying(4),
	NHAug character varying(4),
	NHSep character varying(4),
	NHOct character varying(4),
	NHNov character varying(4),
	NHDec character varying(4),
	SHJan character varying(4),
	SHFeb character varying(4),
